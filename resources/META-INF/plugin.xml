        <action id="FileExportAction21" class="com.github.haifennj.ideaplugin.file.FileExportAction" text="------------------------------">
            <add-to-group group-id="ToolsMenu" anchor="last"/>
        </action>
        <group id="FileExportAction" class="com.github.haifennj.ideaplugin.file.FileExportActionGroup" text="一键导出(更多)" popup="true">
            <add-to-group group-id="ToolsMenu" anchor="last"/>
        </group>
        <group>
            <add-to-group group-id="ToolsMenu" anchor="last"/>
            <separator/>
        </group>
    </actions>

</idea-plugin>